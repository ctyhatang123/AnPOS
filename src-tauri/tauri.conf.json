{
  "$schema": "https://schema.tauri.app/config/2",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../build"
  },
  "package": {
    "productName": "anpos",
    "version": "0.1.0"
  },
  "tauri": {
    "windows": [
      {
        "title": "anpos",
        "width": 800,
        "height": 600
      }
    ],
    "bundle": {
      "identifier": "com.example.anpos",
      "resources": ["inventory.db"]
    }
  }
}